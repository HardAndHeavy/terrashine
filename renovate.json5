{
    "extends": [
        "config:best-practices",
    ],
    "packageRules": [
        {
            "matchPackagePrefixes": [
                "aws-sdk",
            ],
            "matchPackageNames": [
                "aws-config",
                "aws-endpoint",
            ],
            "groupName": "aws-sdk-rust monorepo",
        },
        {
            "matchPackagePrefixes": [
                "tokio",
            ],
            "groupName": "tokio-packages",
        },
    ]
}